<p id="notice"><%= notice %></p>

<h1>In Proceedings</h1>

<table>
  <thead>
    <tr>
      <th>Ref key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Series</th>
      <th>Pages</th>
      <th>Address</th>
      <th>Organization</th>
      <th>Publisher</th>
      <th>Year</th>
      <th>Month</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_proceedings.each do |in_proceeding| %>
      <tr>
        <td><%= in_proceeding.ref_key %></td>
        <td><%= in_proceeding.author %></td>
        <td><%= in_proceeding.title %></td>
        <td><%= in_proceeding.booktitle %></td>
        <td><%= in_proceeding.editor %></td>
        <td><%= in_proceeding.volume %></td>
        <td><%= in_proceeding.number %></td>
        <td><%= in_proceeding.series %></td>
        <td><%= in_proceeding.pages %></td>
        <td><%= in_proceeding.address %></td>
        <td><%= in_proceeding.organization %></td>
        <td><%= in_proceeding.publisher %></td>
        <td><%= in_proceeding.year %></td>
        <td><%= in_proceeding.month %></td>
        <td><%= in_proceeding.note %></td>
        <td><%= link_to 'Show', in_proceeding %></td>
        <td><%= link_to 'Edit', edit_in_proceeding_path(in_proceeding) %></td>
        <td><%= link_to 'Destroy', in_proceeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New In Proceeding', new_in_proceeding_path %>
